<section class="all-asmrtists-table-container text-center">
    <table class="table table-hover w-25">
        <thead>
            <tr>
                <th>ASMRtist</th>
            </tr>
        </thead>
        <tbody>
            <% asmrtists.forEach((asmrtist) => { %>
                <tr>
                    <td><%= asmrtist.name %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</section>